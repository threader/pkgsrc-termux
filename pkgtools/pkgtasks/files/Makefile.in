# Duplicate Automake's check-news target, but verify NEWS.md instead of NEWS.
dist-hook: check-news-local
check-news-local:
	@newsfile="$(srcdir)/NEWS.md"; \
	case `sed 15q "$$newsfile"` in \
	*"$(VERSION)"*) : ;; \
	*) \
		echo "$$newsfile not updated; not releasing" 1>&2; \
		exit 1;; \
	esac

